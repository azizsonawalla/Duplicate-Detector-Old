<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane GridPane.rowIndex="3" GridPane.valignment="CENTER" GridPane.halignment="CENTER" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" percentHeight="50.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" percentHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <AnchorPane id="filePathBox" GridPane.halignment="CENTER" GridPane.valignment="CENTER" GridPane.rowIndex="0" maxHeight="30.0" maxWidth="600.0" prefHeight="30.0" prefWidth="600.0">
            <children>
                <Label fx:id="filePathLabel" id="filePathLabel" styleClass="userInputPrevDefault" text="No folder selected" />
                <Button id="browseButton" AnchorPane.rightAnchor="2.5" AnchorPane.topAnchor="2.5" text="Browse"/>
            </children>
        </AnchorPane>
    </children>
</GridPane>
